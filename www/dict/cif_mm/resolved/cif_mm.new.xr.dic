;
    _type.purpose                SU
    _type.source                 Derived
    _type.container              Single
    _type.contents               Real
    _enumeration.default         ?
    _units.code                  angstrom_squared
    _name.category_id            atom_site_aniso
    _name.object_id              B_11_su
     save_
 
 
